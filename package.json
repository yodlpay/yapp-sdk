{
  "name": "@yodlpay/yapp-sdk",
  "version": "1.7.0",
  "description": "SDK for building Yodl Yapps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yodlpay/yapp-sdk.git"
  },
  "scripts": {
    "build": "tsc && tsc-alias && webpack",
    "build:lib": "tsc && tsc-alias",
    "build:umd": "webpack",
    "prepare": "npm run build",
    "dev": "webpack --watch & node examples/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "typedoc --out docs src/index.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ci": "npm run typecheck && npm run format:check && npm test"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "yodl",
    "yapp",
    "sdk"
  ],
  "author": "Yodl",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typedoc": "^0.27.7",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "jose": "5.x"
  }
}